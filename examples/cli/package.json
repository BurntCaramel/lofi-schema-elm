{
  "name": "lofi-schema",
  "version": "0.1.0",
  "description": "Convert schemas into code",
  "main": "dist/module/lofi-schema.js",
  "bin": {
    "lofi-schema": "dist/cli/lofi-schema"
  },
  "repository": "https://github.com/RoyalIcing/lofi-schema-elm.git",
  "author": "Patrick Smith <patrick@burntcaramel.com>",
  "license": "BSD3",
  "scripts": {
    "build:elm": "elm-make src/Main.elm --output dist/main.js",
    "build:js:module": "microbundle -i src/index.ts -o dist/module -f cjs --no-compress --target node",
    "build:js:cli": "microbundle -i src/cli.ts -o dist/cli -f cjs --no-compress --target node",
    "build:js": "npm run build:js:module && npm run build:js:cli",
    "build": "npm run build:elm && npm run build:js"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "elm": "^0.18.0",
    "microbundle": "^0.4.4"
  },
  "dependencies": {
    "sade": "^1.3.2"
  }
}
